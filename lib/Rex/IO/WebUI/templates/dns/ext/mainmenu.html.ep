<li class="has-sub">
   <a href="#" class="icon-arrow-left">DNS</a>
   <ul class="sub">
      % my $tlds = rexio->get_dns_tlds;
      % for my $tld (@{ $tlds }) {
      % my ($name, $zone);
      % $name = $tld->{name};
      % $zone   = $tld->{zone};
      <li><a href="#" class="dns-link" tld="<%= $zone %>"><%= $name %></a></li>
      % }
   </ul>
</li>

