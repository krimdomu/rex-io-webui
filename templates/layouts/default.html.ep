<!DOCTYPE html>
<html>
   <head>
      <title>Rex::IO::WebUI - Dashboard</title>
      <script src="http://ajax.googleapis.com/ajax/libs/dojo/1.7.2/dojo/dojo.js"
         data-dojo-config="async: true, parseOnLoad: true"></script>
      <script src="/javascripts/webui.js"></script>
      <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/dojo/1.7.2/dijit/themes/claro/claro.css" media="screen">
      <link rel="stylesheet" href="/stylesheets/webui.css" type="text/css" media="screen" charset="utf-8">
   </head>
   <body class="claro">
      <div id="appLayout" class="demoLayout" data-dojo-type="dijit.layout.BorderContainer" data-dojo-props="design: 'headline'">

         <!-- content bereich -->
         <div class="centerPanel" data-dojo-type="dijit.layout.TabContainer" data-dojo-props="region: 'center', tabPosition: 'bottom'">
            <div data-dojo-type="dijit.layout.ContentPane" title="Desktop">
               <h4>Group 1 Content</h4>
               <p>center bereich</p>
            </div>
         </div>

         <!-- header (menu) -->
         <div class="edgePanel" data-dojo-type="dijit.layout.ContentPane" data-dojo-props="region: 'top'">
            Header content (top)
         </div>

         <!-- sidebar -->
         <div id="leftCol" class="edgePanel" data-dojo-type="dijit.layout.ContentPane" data-dojo-props="region: 'left', splitter: true">
            <div data-dojo-type="dijit.layout.AccordionContainer">
               <div data-dojo-type="dijit.layout.ContentPane" data-dojo-props="title:'Servers'">
                  <div id="tree"></div>
               </div>
               <div data-dojo-type="dijit.layout.ContentPane" data-dojo-props="title:'Services'">
               </div>
            </div>
         </div>

		</div>
   </body>
</html>
